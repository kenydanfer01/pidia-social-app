{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Enfermedad Asociada' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'enfermedad_asociada_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',enfermedad_asociada.nombre) }}
