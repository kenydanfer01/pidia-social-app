    {{ table.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'enfermedad_asociada','entity': enfermedad_asociada} %}
{% endblock %}

{% block _body_tools %}
