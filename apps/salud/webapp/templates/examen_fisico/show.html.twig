{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Examen Fisico' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'examen_fisico_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Peso',examen_fisico.peso) }}
    {{ table.tbl_tr('Talla',examen_fisico.talla) }}
    {{ table.tbl_tr('Presion Arterial',examen_fisico.presionArterial) }}
    {{ table.tbl_tr('Frecuencia Cardiaca',examen_fisico.frecuenciaCardiaca) }}
    {{ table.tbl_tr('Temperatura',examen_fisico.temperatura) }}
    {{ table.tbl_tr('Frecuencia Respiratoria',examen_fisico.frecuenciaRespiratoria) }}
    {{ table.tbl_tr('IMC',examen_fisico.imc) }}
    {{ table.tbl_tr('Observaciones',examen_fisico.observaciones) }}
    {% include 'crud/_show_info.html.twig' with {'entity': examen_fisico} %}
    {{ table.tbl_end() }}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'examen_fisico','entity': examen_fisico} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'examen_fisico','entity': examen_fisico} %}
{% endblock %}
