{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Base Social' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'base_social_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',base_social.nombre) }}
    {{ table.tbl_tr('Localidad',base_social.localidad) }}
    {% include 'crud/_show_info.html.twig' with {'entity': base_social} %}
    {{ table.tbl_end() }}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'base_social','entity': base_social} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'base_social','entity': base_social} %}
{% endblock %}
