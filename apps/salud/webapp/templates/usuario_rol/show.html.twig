{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Rol de Usuario' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'usuario_rol_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',usuario_rol.name) }}
    {{ table.tbl_tr('Rol',usuario_rol.rol) }}
    {% include 'crud/_show_info.html.twig' with {'entity': usuario_rol} %}
    {{ table.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'usuario_rol','entity': usuario_rol} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'usuario_rol','entity': usuario_rol} %}
{% endblock %}
