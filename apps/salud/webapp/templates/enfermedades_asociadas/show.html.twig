{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Enfermedades Asociadas' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'enfermedades_asociadas_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',enfermedades_asociadas.nombre) }}
    {% include 'crud/_show_info.html.twig' with {'entity': enfermedades_asociadas} %}
    {{ table.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'enfermedades_asociadas','entity': enfermedades_asociadas} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'enfermedades_asociadas','entity': enfermedades_asociadas} %}
{% endblock %}
