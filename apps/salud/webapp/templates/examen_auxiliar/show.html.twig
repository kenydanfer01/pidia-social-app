{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Examen Auxiliar' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'examen_auxiliar_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',examen_auxiliar.nombre) }}
    {{ table.tbl_tr('Alias',examen_auxiliar.alias) }}
    {% include 'crud/_show_info.html.twig' with {'entity': examen_auxiliar} %}
    {{ table.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'examen_auxiliar','entity': examen_auxiliar} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'examen_auxiliar','entity': examen_auxiliar} %}
{% endblock %}
