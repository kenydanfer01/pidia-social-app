{% import 'theme/form.html.twig' as frm %}

{{ frm.start(form) }}
    {{ frm.columnX(
        [
            frm.row(form.dni),
            frm.row(form.apellidoPaterno),
            frm.row(form.apellidoMaterno),
            frm.row(form.nombres),
            frm.row(form.direccion),
            frm.row(form.telefono),
            frm.row(form.fechaNacimiento),
            frm.row(form.estadoCivil),
            frm.row(form.sexo),
        ],
        4
    ) }}
    {{ frm.columnX(
        [
            frm.row(form.baseSocial),
            frm.row(form.posicion),
        ],
        4
    ) }}
    <div id="titular_txt">
        {{ frm.columnX(
            [
                frm.row(form.titular),
            ],
            4
        ) }}
    </div>
    {{ frm.button(button_label|default('Guardar')) }}
{{ frm.end(form) }}
