{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Paciente' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'paciente_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Nombre',paciente.apellidoPaterno~' '~paciente.apellidoMaterno~' '~paciente.nombres) }}
    {{ table.tbl_tr('Dni',paciente.dni) }}
    {{ table.tbl_tr('Posici√≥n',paciente.posicion) }}
    {% include 'crud/_show_info.html.twig' with {'entity': paciente} %}
    {{ table.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'paciente','entity': paciente} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'paciente','entity': paciente} %}
{% endblock %}
