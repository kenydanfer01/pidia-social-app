{% extends 'base.html.twig' %}
{% import '@App/theme1/breadcrumb.html.twig' as breadcrumb %}
{% import '@App/theme1/table.html.twig' as table %}

{% set _title = 'Proyeccion' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'proyeccion_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ table.tbl_start() }}
    {{ table.tbl_tr('Socio',proyeccion.socio) }}
    {{ table.tbl_tr('AÃ±o',proyeccion.anio) }}
    {{ table.tbl_tr('Proyeccion',proyeccion.quintales) }}
    {{ table.tbl_tr('Acopio',proyeccion.acopiadoQuintales) }}
    {{ table.tbl_tr('Aporte','S/' ~  proyeccion.aporte) }}
    {{ table.tbl_tr('Pago Aporte','S/' ~  proyeccion.pagoAporte) }}
    {% include 'crud/_show_info.html.twig' with {'entity': proyeccion} %}
    {{ table.tbl_end() }}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'proyeccion','entity': proyeccion} %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'proyeccion','entity': proyeccion} %}
{% endblock %}
