{
    "name": "carloschininin/social-ddd",
    "description": "Social DDD",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Carlos Chininin",
            "email": "carloschininin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "carloschininin/app": "dev-main",
        "carloschininin/data": "dev-master",
        "carloschininin/file-upload-bundle": "dev-main",
        "carloschininin/util": "dev-master",
        "carloschininin/api-search-person": "^0.0.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.11",
        "liip/imagine-bundle": "3.x-dev",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "^6.1",
        "symfony/console": "^6.1",
        "symfony/doctrine-messenger": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/expression-language": "^6.1",
        "symfony/form": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/mailer": "^6.1",
        "symfony/messenger": "^6.1",
        "symfony/mime": "^6.1",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^6.1",
        "symfony/property-access": "^6.1",
        "symfony/property-info": "^6.1",
        "symfony/proxy-manager-bridge": "^6.1",
        "symfony/runtime": "^6.1",
        "symfony/security-bundle": "^6.1",
        "symfony/serializer": "^6.1",
        "symfony/string": "^6.1",
        "symfony/translation": "^6.1",
        "symfony/twig-bundle": "^6.1",
        "symfony/uid": "^6.1",
        "symfony/validator": "^6.1",
        "symfony/yaml": "^6.1",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SocialApp\\Apps\\Security\\Webapp\\": "apps/security/webapp/src",
            "SocialApp\\Apps\\Demo\\Webapp\\": "apps/demo/webapp/src",
            "SocialApp\\Apps\\Salud\\Webapp\\": "apps/salud/webapp/src",
            "SocialApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SocialApp\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "symfony/symfony": "v6.2.7"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.2.7"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpunit/phpunit": "^9.5",
        "qossmic/deptrac-shim": "^1.0",
        "symfony/browser-kit": "^6.1",
        "symfony/css-selector": "^6.1",
        "symfony/debug-bundle": "^6.1",
        "symfony/http-client": "^6.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "^6.1",
        "symfony/var-dumper": "^6.2",
        "symfony/web-profiler-bundle": "^6.1"
    }
}
